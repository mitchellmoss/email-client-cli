[run]
source = .
omit = 
    */tests/*
    */test_*
    */venv/*
    */migrations/*
    */__pycache__/*
    */config.py
    main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = htmlcov